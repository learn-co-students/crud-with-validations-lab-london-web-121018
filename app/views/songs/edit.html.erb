<h1>Edit Song</h1>

 <%= form_for @song do |t| %>
 <%= t.label :title %>
 <%= t.text_field :title %>
 <br>
  <br>
 <%= t.label :released %>
 <%= t.check_box :released %>
 <br>
  <br>
 <%= t.label :release_year %>
 <%= t.text_field :release_year %>
 <br>
  <br>
 <%= t.label :artist_name %>
 <%= t.text_field :artist_name %>
<br>
 <br>
 <%= t.label :genre %>
 <%= t.text_field :genre %>
 <br>
  <br>
 <%= t.submit %>
 <% end %>

 <%= link_to "Delete", @song, method: :delete, data: { confirm: "Really?" } %>
